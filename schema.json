          type: string
          maxLength: 200
        body:
          type: string
      required:
      - body
      - id
      - title
      - user_id
  securitySchemes:
    basicAuth:
      type: http
      scheme: basic
    cookieAuth:
      type: apiKey
      in: cookie
      name: sessionid
