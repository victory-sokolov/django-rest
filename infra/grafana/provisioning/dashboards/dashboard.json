{
   "description": "Djago App Metrics",
   "panels": [
      {
         "collapsed": false,
         "gridPos": {
            "h": 1,
            "w": 24,
            "x": 0,
            "y": 0
         },
         "id": 1,
         "panels": [ ],
         "title": "Haproxy",
         "type": "row"
      },
      {
         "datasource": {
            "type": "$datasource",
            "uid": "$datasource"
         },
         "description": "Haproxy Frontend Sessions",
         "fieldConfig": {
            "defaults": {
               "custom": {
                  "fillOpacity": 10,
                  "showPoints": "never"
               },
               "unit": "none"
            }
         },
         "gridPos": {
            "h": 8,
            "w": 12,
            "x": 0,
            "y": 1
         },
         "id": 2,
         "interval": "1m",
         "options": {
            "legend": {
               "calcs": [
                  "lastNotNull",
                  "max"
               ],
               "displayMode": "table"
            }
         },
         "pluginVersion": "v11.1.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "sum(\n    haproxy_frontend_current_sessions{job=~\"$job\", proxy=\"http-in\", instance=~\"$instance\"}\n) or vector(0)\n",
               "intervalFactor": 2,
               "legendFormat": "Total sessions by (frontend)"
            }
         ],
         "title": "Frontend Sessions",
         "type": "timeseries"
      },
      {
         "datasource": {
            "type": "$datasource",
            "uid": "$datasource"
         },
         "description": "Haproxy Backend Sessions",
         "fieldConfig": {
            "defaults": {
               "custom": {
                  "fillOpacity": 10,
                  "showPoints": "never"
               },
               "unit": "none"
            }
         },
         "gridPos": {
            "h": 8,
            "w": 12,
            "x": 12,
            "y": 1
         },
         "id": 3,
         "interval": "1m",
         "options": {
            "legend": {
               "calcs": [
                  "lastNotNull",
                  "max"
               ],
               "displayMode": "table"
            }
         },
         "pluginVersion": "v11.1.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "sum(\n    haproxy_backend_current_sessions{job=\"haproxy-metrics\", instance=~\"$instance\", proxy=\"server_backend\"}\n) or vector(0)\n",
               "intervalFactor": 2,
               "legendFormat": "Total sessions by (backend)"
            }
         ],
         "title": "Backend Sessions",
         "type": "timeseries"
      },
      {
         "datasource": {
            "type": "$datasource",
            "uid": "$datasource"
         },
         "description": "HAProxy Requests",
         "fieldConfig": {
            "defaults": {
               "custom": {
                  "fillOpacity": 10,
                  "showPoints": "never"
               },
               "unit": "none"
            }
         },
         "gridPos": {
            "h": 8,
            "w": 12,
            "x": 0,
            "y": 9
         },
         "id": 4,
         "interval": "1m",
         "options": {
            "legend": {
               "calcs": [
                  "lastNotNull",
                  "max"
               ],
               "displayMode": "table"
            }
         },
         "pluginVersion": "v11.1.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "sum(\n    rate(\n        haproxy_frontend_http_requests_total{job=~\"$job\", proxy=\"http-in\", instance=~\"$instance\"}[$__rate_interval]\n    )\n) by (code) or vector(0)\n",
               "intervalFactor": 2,
               "legendFormat": "{{code}}"
            }
         ],
         "title": "HAProxy Requests",
         "type": "timeseries"
      },
      {
         "datasource": {
            "type": "$datasource",
            "uid": "$datasource"
         },
         "description": "Haproxy Average Response Time",
         "fieldConfig": {
            "defaults": {
               "custom": {
                  "fillOpacity": 10,
                  "showPoints": "never"
               },
               "unit": "none"
            }
         },
         "gridPos": {
            "h": 8,
            "w": 12,
            "x": 12,
            "y": 9
         },
         "id": 5,
         "interval": "1m",
         "options": {
            "legend": {
               "calcs": [
                  "lastNotNull",
                  "max"
               ],
               "displayMode": "table"
            }
         },
         "pluginVersion": "v11.1.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "avg(\n haproxy_backend_response_time_average_seconds{job=~\"$job\", proxy=\"server_backend\", instance=~\"$instance\"}\n) by (proxy)\n",
               "legendFormat": "{{ proxy }} Response Time\n"
            }
         ],
         "title": "Average Response Time",
         "type": "timeseries"
      },
      {
         "datasource": {
            "type": "$datasource",
            "uid": "$datasource"
         },
         "description": "Haproxy Status Codes",
         "fieldConfig": {
            "defaults": {
               "custom": {
                  "fillOpacity": 10,
                  "showPoints": "never"
               },
               "unit": "none"
            }
         },
         "gridPos": {
            "h": 8,
            "w": 12,
            "x": 0,
            "y": 17
         },
         "id": 6,
         "interval": "1m",
         "options": {
            "legend": {
               "calcs": [
                  "lastNotNull",
                  "max"
               ],
               "displayMode": "table"
            }
         },
         "pluginVersion": "v11.1.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "sum(\n    rate(haproxy_frontend_http_responses_total{\n        job=\"haproxy-metrics\",\n        proxy=\"http-in\",\n        instance=~\"$instance\"\n    }[$__rate_interval])\n) by (code) or vector(0)\n",
               "legendFormat": "Frontend {{ code }}\n"
            }
         ],
         "title": "Frontend Status Codes",
         "type": "timeseries"
      },
      {
         "collapsed": false,
         "gridPos": {
            "h": 1,
            "w": 24,
            "x": 0,
            "y": 25
         },
         "id": 7,
         "panels": [ ],
         "title": "Django",
         "type": "row"
      },
      {
         "datasource": {
            "type": "$datasource",
            "uid": "$datasource"
         },
         "description": "Django DB Query Errors",
         "fieldConfig": {
            "defaults": {
               "unit": "none"
            }
         },
         "gridPos": {
            "h": 8,
            "w": 12,
            "x": 0,
            "y": 26
         },
         "id": 8,
         "interval": "1m",
         "pluginVersion": "v11.1.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "sum(\n    max_over_time(\n        django_db_errors_total{\n            instance=~\"$instance\",\n            job=~\"$job\",\n        }[$__rate_interval]\n    )\n)\n",
               "intervalFactor": 2,
               "legendFormat": "Django ORM DB errors count\n"
            }
         ],
         "title": "DB Query Errors",
         "type": "gauge"
      },
      {
         "datasource": {
            "type": "$datasource",
            "uid": "$datasource"
         },
         "description": "Django Database Connection Errors",
         "fieldConfig": {
            "defaults": {
               "unit": "none"
            }
         },
         "gridPos": {
            "h": 8,
            "w": 12,
            "x": 12,
            "y": 26
         },
         "id": 9,
         "interval": "1m",
         "pluginVersion": "v11.1.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "sum(\n    max_over_time(\n        django_db_new_connection_errors_total{\n            instance=~\"$instance\",\n            job=~\"$job\",\n        }[$__rate_interval]\n    )\n)\n",
               "intervalFactor": 2,
               "legendFormat": "Django ORM DB errors count\n"
            }
         ],
         "title": "DB Connection Errors",
         "type": "gauge"
      },
      {
         "datasource": {
            "type": "$datasource",
            "uid": "$datasource"
         },
         "description": "Django Request Latency",
         "fieldConfig": {
            "defaults": {
               "custom": {
                  "fillOpacity": 10,
                  "showPoints": "never"
               },
               "unit": "none"
            }
         },
         "gridPos": {
            "h": 8,
            "w": 12,
            "x": 0,
            "y": 34
         },
         "id": 10,
         "interval": "1m",
         "options": {
            "legend": {
               "calcs": [
                  "lastNotNull",
                  "max"
               ],
               "displayMode": "table"
            }
         },
         "pluginVersion": "v11.1.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "histogram_quantile(\n    0.50,\n    sum(irate(\n        django_http_requests_latency_seconds_by_view_method_bucket{\n            instance=~\"$instance\",\n            job=~\"$job\"\n        }[$__rate_interval]\n    )) by (le)\n) or vector(0)\n",
               "intervalFactor": 2,
               "legendFormat": "Django Request latency\n"
            }
         ],
         "title": "Django Request Latency",
         "type": "timeseries"
      },
      {
         "datasource": {
            "type": "$datasource",
            "uid": "$datasource"
         },
         "description": "Django Response Status",
         "fieldConfig": {
            "defaults": {
               "custom": {
                  "fillOpacity": 10,
                  "showPoints": "never"
               },
               "unit": "none"
            }
         },
         "gridPos": {
            "h": 8,
            "w": 12,
            "x": 12,
            "y": 34
         },
         "id": 11,
         "interval": "1m",
         "options": {
            "legend": {
               "calcs": [
                  "lastNotNull",
                  "max"
               ],
               "displayMode": "table"
            }
         },
         "pluginVersion": "v11.1.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "sum(\n    increase(\n        django_http_responses_total_by_status_total{\n            instance=~\"$instance\",\n            job=~\"$job\",\n        }[$__rate_interval]\n    )\n) by (status) or vector(0)\n",
               "intervalFactor": 2,
               "legendFormat": "{{status}}\n"
            }
         ],
         "title": "Django Response Status",
         "type": "timeseries"
      },
      {
         "datasource": {
            "type": "$datasource",
            "uid": "$datasource"
         },
         "description": "Gunicorn request duration",
         "fieldConfig": {
            "defaults": {
               "custom": {
                  "fillOpacity": 10,
                  "showPoints": "never"
               },
               "unit": "none"
            }
         },
         "gridPos": {
            "h": 8,
            "w": 12,
            "x": 0,
            "y": 42
         },
         "id": 12,
         "interval": "1m",
         "options": {
            "legend": {
               "calcs": [
                  "lastNotNull",
                  "max"
               ],
               "displayMode": "table"
            }
         },
         "pluginVersion": "v11.1.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "sum(\n    app_gunicorn_request_duration{\n        instance=~\"$instance\",\n        job=~\"$job\"\n    } or vector(0)\n)\n",
               "intervalFactor": 2,
               "legendFormat": "Request Duration"
            }
         ],
         "title": "Gunicorn request duration",
         "type": "timeseries"
      },
      {
         "datasource": {
            "type": "$datasource",
            "uid": "$datasource"
         },
         "description": "Django Database Total Queries",
         "fieldConfig": {
            "defaults": {
               "custom": {
                  "fillOpacity": 10,
                  "showPoints": "never"
               },
               "unit": "none"
            }
         },
         "gridPos": {
            "h": 8,
            "w": 12,
            "x": 12,
            "y": 42
         },
         "id": 13,
         "interval": "1m",
         "options": {
            "legend": {
               "calcs": [
                  "lastNotNull",
                  "max"
               ],
               "displayMode": "table"
            }
         },
         "pluginVersion": "v11.1.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "sum(\n    irate(django_db_execute_total{instance=~\"$instance\", job=~\"$job\",}[$__rate_interval])\n) by (vendor) or vector(0)\n",
               "intervalFactor": 2,
               "legendFormat": "Database Total Queries\n"
            }
         ],
         "title": "DB Total Queries",
         "type": "timeseries"
      },
      {
         "datasource": {
            "type": "$datasource",
            "uid": "$datasource"
         },
         "description": "Cache Hit Ratio",
         "fieldConfig": {
            "defaults": {
               "unit": "none"
            }
         },
         "gridPos": {
            "h": 8,
            "w": 12,
            "x": 0,
            "y": 50
         },
         "id": 14,
         "interval": "1m",
         "pluginVersion": "v11.1.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "sum(\n    django_cache_get_hits_total{\n        instance=~\"$instance\",\n        job=~\"$job\"\n    }) by (backend) / sum(django_cache_get_total{\n        instance=~\"$instance\",\n        job=~\"$job\"\n    }) by (backend)\n",
               "intervalFactor": 2,
               "legendFormat": "Cache Hit Ratio"
            }
         ],
         "title": "Cache Hit Ratio",
         "type": "gauge"
      },
      {
         "collapsed": false,
         "gridPos": {
            "h": 1,
            "w": 24,
            "x": 0,
            "y": 58
         },
         "id": 15,
         "panels": [ ],
         "title": "PostgreSQL",
         "type": "row"
      },
      {
         "datasource": {
            "type": "$datasource",
            "uid": "$datasource"
         },
         "description": "Shows the maximum number of concurrent connections to the PostgreSQL database",
         "fieldConfig": {
            "defaults": {
               "unit": "none"
            }
         },
         "gridPos": {
            "h": 8,
            "w": 12,
            "x": 0,
            "y": 59
         },
         "id": 16,
         "interval": "1m",
         "pluginVersion": "v11.1.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "pg_settings_max_connections{job=~\"$job\", instance=~\"$instance\"}",
               "intervalFactor": 2,
               "legendFormat": "PostgreSQL max_connections"
            }
         ],
         "title": "Max connections",
         "type": "stat"
      }
   ],
   "schemaVersion": 39,
   "templating": {
      "list": [
         {
            "includeAll": true,
            "label": "Data source",
            "name": "datasource",
            "query": "prometheus",
            "type": "datasource"
         },
         {
            "allValue": ".+",
            "datasource": {
               "type": "prometheus",
               "uid": "${datasource}"
            },
            "includeAll": true,
            "label": "Job",
            "multi": true,
            "name": "job",
            "query": "label_values(job)",
            "refresh": 2,
            "type": "query"
         },
         {
            "current": {
               "selected": false,
               "text": "30s",
               "value": "30s"
            },
            "description": "Rate",
            "label": "Rate",
            "name": "rate",
            "options": [
               {
                  "selected": true,
                  "text": "30s",
                  "value": "30s"
               },
               {
                  "selected": false,
                  "text": "1m",
                  "value": "1m"
               },
               {
                  "selected": false,
                  "text": "10m",
                  "value": "10m"
               },
               {
                  "selected": false,
                  "text": "15m",
                  "value": "15m"
               },
               {
                  "selected": false,
                  "text": "30m",
                  "value": "30m"
               },
               {
                  "selected": false,
                  "text": "1h",
                  "value": "1h"
               },
               {
                  "selected": false,
                  "text": "6h",
                  "value": "6h"
               },
               {
                  "selected": false,
                  "text": "12h",
                  "value": "12h"
               }
            ],
            "query": "30s : 30s,1m : 1m,10m : 10m,15m : 15m,30m : 30m,1h : 1h,6h : 6h,12h : 12h",
            "type": "custom"
         },
         {
            "allValue": ".+",
            "datasource": {
               "type": "prometheus",
               "uid": "${datasource}"
            },
            "includeAll": true,
            "label": "Instance",
            "multi": true,
            "name": "instance",
            "query": "label_values({job=~\"$job\"}, instance)",
            "refresh": 2,
            "type": "query"
         }
      ]
   },
   "time": {
      "from": "now-6h",
      "to": "now"
   },
   "timezone": "utc",
   "title": "Django App Dashboard"
}
